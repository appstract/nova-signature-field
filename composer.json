{
    "name": "appstract/nova-signature-field",
    "description": "A Laravel Nova field with Signature Pad",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\NovaSignatureField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appstract\\NovaSignatureField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
